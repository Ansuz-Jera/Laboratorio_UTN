{
  "name": "laboratorio_utn",
  "version": "1.0.0",
  "description": "Página para guardar pedidos y otros datos del laboratorio",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss ",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin contacts.html -d dist --htmlmin -o dist/contacts.html && usemin orders.html -d dist --htmlmin -o dist/orders.html && usemin repair.html -d dist --htmlmin -o dist/repair.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ansuz-Jera/laboratorio_utn.git"
  },
  "author": "Azurmendi Joaquín",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ansuz-Jera/laboratorio_utn/issues"
  },
  "homepage": "https://github.com/Ansuz-Jera/laboratorio_utn#readme",
  "dependencies": {
    "bootstrap": "^4.5.3",
    "concurrently": "^5.3.0",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "sass": "^1.30.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  }
}
